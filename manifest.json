{
  "manifest_version": 4,
  "name": "Twitter Logo Changer",
  "version": "2.0",
  "description": "No more X, Replace Twitter logo to classic.",
  "action": {
    "default_icon": {
      "48": "logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "48": "logo.png"
  }
}
